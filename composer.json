{
    "name": "signify-nz/composer-security-checker",
    "description": "A security checker for your composer.lock",
    "license": "BSD-3-Clause",
    "keywords": [
        "composer",
        "security",
        "checker"
    ],
    "authors": [
        {
            "name": "Signify Ltd",
            "homepage": "https://www.signify.co.nz/"
        }
    ],
    "support": {
        "issues": "https://github.com/signify-nz/composer-security-checker/issues"
    },
    "require": {
        "php": ">= 7.3.0",
        "ext-json": "*",
        "ext-zip": ">= 1.1.0",
        "guzzlehttp/guzzle": "^6 || ^7",
        "symfony/yaml": "^3.2 || ^4 || ^5",
        "composer/semver": "^1 || ^2 || ^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Signify\\SecurityChecker\\": "src/",
            "Signify\\SecurityChecker\\Tests\\": "tests/"
        }
    }
}
